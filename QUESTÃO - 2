//Somar Dois NÃºmeros
#include <stdio.h>

int main(){
//Variaveis
int num1, num2, resultado;

//Algoritmo para imprimir os numeros a serem somados
 printf("Digite dois numeros para serem somados\n");
  scanf("%i", &num1); 
 printf("+\n");
  scanf("%i", &num2);

//Algoritmo para realizar a soma 
 resultado = num1 + num2;
 printf("A soma dos dois numeros e: \n %i", resultado);
return 0;
}
